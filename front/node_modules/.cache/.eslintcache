ets/instagram.jpg'\nimport { Link } from \"react-router-dom\"\nimport camera from '../Assets/camera.jpeg'\nimport aboutImage from '../Assets/about.jpg'\nimport phone from '../Assets/phone.png'\nimport imagix from '../Assets/imagixlogo.jpg'\nimport eleli from '../Assets/elelilogo.jpg'\n\nfunction Homepage() {\n  return (\n    <div className='homePageWrapper'>\n        <div className=\"landingPage\">\n            <div className=\"header\">\n                <div className=\"hTittle\">\n                <Link to='/'> <span>IMAGIX <br /> MEDIA</span> </Link> \n                </div>\n                <div className=\"socials\">\n                    <div className=\"links\">\n                        <a target=\"_blank\" href=\"https://www.facebook.com/scola.imagix\"><img src={facebook} alt=\"\" srcset=\"\" /></a>\n                        <a target=\"_blank\" href=\"https://instagram.com/imagixafrica?igshid=YmMyMTA2M2Y\"><img src={instagram} alt=\"\" srcset=\"\" /></a>\n                    </div>\n                    <div className=\"phone\">\n                        <span>0702659667</span>\n                    </div>\n                </div>\n            </div>\n            <div className=\"body\">\n                  \n               <div className=\"canva\">\n               \n                   <img src={camera} alt=\"\" />\n                   <div className=\"text\">\n                       <span>IMAGIX</span>\n                       <span>MEDIA</span>\n                   </div>\n                    <div className=\"right\"></div>\n                    \n                    <div className=\"buttons\">\n                        <span> <Link to='/gallery'>Gallery</Link></span>\n                        <span><Link to='/rates'>Rates</Link></span>\n                        <span><Link to='/gallery'>Works</Link></span>\n                        <span><Link to='/rates'>Book Now</Link></span>\n                    </div>\n               </div>\n               <div className=\"pageTwo\">\n                    <div className=\"topHalf\">\n                        <h2>About Us</h2>\n                        <img src={aboutImage} alt=\"\" />\n                    </div>\n                    <div className=\"about\">\n                        <h3>Imagix</h3>\n                        <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Illo exercitationem praesentium labore ut mollitia nam consequatur eveniet nobis. Nesciunt adipisci voluptatum, consectetur quibusdam veniam recusandae beatae quos assumenda quia amet!</p>\n                        <a href=\"\">Book Us</a>\n                    </div>\n               </div>\n               <div className=\"pageThree\">\n                    <h2>We Cover The Following</h2>\n                    <div className=\"categories\">\n                        <span>Weddings</span>\n                        <span>Events</span>\n                        <span>Potraits</span>\n                        <span>Birthdays</span>\n                        <span>Baby Bumps</span>\n                        <span>Ruracios</span>\n                        <span>Corporate Meetings</span>\n                        <span>Rallies</span>\n                    </div>\n                    <div className=\"desc\">\n                        <p>We shoot Proffesional Images and videos in all of the above.Click Button below to book us or see our rates</p>\n                        <div className=\"links\">\n                            <a href=\"\">Rates</a>\n                            <a href=\"\">Book Now</a>\n                        </div>\n                    </div>\n               </div>\n               <div className=\"pageFour\" id='contacts'>\n                    <h2>Contact Us Via</h2>\n                    <div className=\"links\">\n                        <a target=\"_blank\" href=\"https://www.facebook.com/scola.imagix\"><img src={facebookB} alt=\"\" /></a>\n                        <a target=\"_blank\" href=\"https://instagram.com/imagixafrica?igshid=YmMyMTA2M2Y\"><img src={instagramB} alt=\"\" /></a>\n                        <a target=\"_blank\" href=\"\"><img src={gmailB} alt=\"\" /></a>\n                    </div>\n                    <div className=\"phone\">\n                        <img src={phone} alt=\"\" />\n                        <p>07 02 659 667</p>\n                        \n                    </div>\n\n                    <div className='end'>Find us on all social medias, alias imagix africa</div>\n                    <div className=\"footer\">\n                        <span>\n                            <h4>Made For</h4>\n                            <img src={imagix} alt=\"\" />\n                        </span>\n                        <span>\n                            <h4>Made By</h4>\n                            <img src={eleli} alt=\"\" />\n                        </span>\n                    </div>\n               </div>\n            </div>\n\n        </div>\n       \n    </div>\n  )\n}\n\nexport default Homepage","/home/eleli/Documents/Projects/Web Projects/imagix-media/front/src/Rates/Rates.js",["64","65"],[],"import React from 'react'\nimport './Rates.scss'\nimport facebook from '../Assets/facebook.png'\nimport instagram from '../Assets/instagram.jpg'\nimport { Link } from \"react-router-dom\"\nimport RateCard from './RateCard'\n\nfunction Rates() {\n  return (\n    <div className='ratesWrapper'>\n        <div className=\"header\">\n                <div className=\"hTittle\">\n                <Link to='/'> <span>IMAGIX <br /> MEDIA</span> </Link> \n                </div>\n                <div className=\"socials\">\n                    <div className=\"links\">\n                        <a target=\"_blank\" href=\"https://www.facebook.com/scola.imagix\"><img src={facebook} alt=\"\" srcset=\"\" /></a>\n                        <a target=\"_blank\" href=\"https://instagram.com/imagixafrica?igshid=YmMyMTA2M2Y\"><img src={instagram} alt=\"\" srcset=\"\" /></a>\n                    </div>\n                    <div className=\"phone\">\n                        <span>0702659667</span>\n                    </div>\n                </div>\n        </div>\n        <h2>Rates</h2>\n        <div className=\"rateCards\">\n            <RateCard title=\"PROFESSIONAL WEDDING PHOTOGRAPHY and VIDEOGRAPHY\" imagesNo=\"Unlimited Number of Images\" duration=\"Duration of The Whole Event\" other=\"Images available for digital download\" price=\"35-65\" />\n            <RateCard title=\"OUTDOOR PHOTOGRAPHY\" imagesNo=\"10 Edited Images\" duration=\"One Hour Photo Session\" other=\"Session at Location of Your Choice Within Nairobi\" price=\"3, 000\" />\n            <RateCard title=\"SHOOT FOR TWO\" imagesNo=\"15 Edited Images\" duration=\"Three Hours Photo Session\" other=\"Images Available Digitally in Google Drive\" price=\"5, 000\" />\n            <RateCard title=\"FAMILY PHOTO-SHOOT\" imagesNo=\"15 Edited Images\" duration=\"Four Hours Photo Session\" other=\"Location of Your Choice Within Nairobi\" price=\"3, 500\" />\n            <RateCard title=\"BABY BUMP / ENGAGEMENT\" imagesNo=\"15 Edited Images\" duration=\"Four Hours Photo Session\" other=\"Free Three Clothing Change\" price=\"7, 000\" />\n            <RateCard title=\"EVENTS\" imagesNo=\"Full Event Coverage\" duration=\"Five to Seven Hours Event Coverage\" other=\"All Images Available for Digital Download Via Google Drive\" price=\"15, 000\" />\n            <RateCard title=\"PRODUCT PHOTOGRAPHY\" imagesNo=\"10 Edited Images\" duration=\"Three Hours Photo Session\" other=\"Freedom of Location Within Nairobi\" price=\"2, 000\" />\n            \n        </div>\n    \n        <div className=\"info\">\n            <h1>NOTE</h1>\n            <p>Clients are expected to pay non- refundable deposit at least 2 days before the intended day of the shoot. <br /> For outdoor shots, the client is to provide the location and if be any expenses incurred for using the location will be on client. <br /> Kindly note that time allocated to you for your session is strict. Any additional time will be charged</p>\n        </div>\n    </div>\n  )\n}\n\nexport default Rates","/home/eleli/Documents/Projects/Web Projects/imagix-media/front/src/Rates/RateCard.js",[],[],"/home/eleli/Documents/Projects/Web Projects/imagix-media/front/src/Gallery/Galleryy.js",["66","67","68"],[],"// Importing the necessary dependencies\nimport React, { useEffect, useState } from 'react'\nimport { Link } from \"react-router-dom\"\nimport './Gallery.scss'\nimport { storage } from \"./firebase\";\nimport { ref, uploadBytes, getDownloadURL, listAll} from 'firebase/storage'\nimport { v4 } from 'uuid'\n\n\n//Import the needed images\nimport facebook from '../Assets/facebook.png'\nimport instagram from '../Assets/instagram.jpg'\nimport inputImg from '../Assets/image.png'\n\nfunction Gallery() {\n\n    //create states for files and image urls\n    const [imageUpload, setImageUpload] = useState(null)\n    const [imageUrls, setImageUrls] = useState([])\n\n    const imagesListRef = ref(storage, 'imagix-media/')\n\n    //Function to upload file on button click\n    const uploadImage = ()=>{\n        if (imageUpload == null) {\n            console.log('Image not present. Try again');\n            return;\n        }else{\n            const imageRef = ref(storage, `imagix-media/${imageUpload.name + v4()}`)\n            uploadBytes(imageRef, imageUpload).then((snapshot)=>{\n                alert(\"Image added\")\n                getDownloadURL(snapshot.ref).then((url)=>{\n                    setImageUrls((prev)=>[...prev, url])\n                })\n            })\n        }\n    }\n\n\n    useEffect(()=>{\n        listAll(imagesListRef).then((response)=>{\n            response.items.forEach((item)=>{\n                getDownloadURL(item).then((url)=>{\n                    setImageUrls((prev)=>[...prev, url])\n                    console.log(imageUrls);\n                })\n            })\n        })\n    },[])\n\n    \n  retur